{
    "name": "@form-create/designer",
    "version": "1.1.0",
    "description": "好用的vue可视化表单设计器组件",
    "type": "module",
    "unpkg": "./dist/index.umd.js",
    "jsdelivr": "./dist/index.umd.js",
    "typings": "./types/index.d.ts",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "scripts": {
        "dev": "vite --config vite.dev.config.js",
        "build": "vite build",
        "preview": "vite preview"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xaboy/form-create-designer.git"
    },
    "keywords": [
        "表单设计器",
        "@form-create",
        "form-builder",
        "form-designer",
        "draggable",
        "form",
        "components",
        "vue.js",
        "element-ui",
        "json-form",
        "dynamic-form"
    ],
    "files": [
        "README.md",
        "CHANGELOG.md",
        "package.json",
        "LICENSE",
        "src",
        "types",
        "dist"
    ],
    "author": "xaboy",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xaboy/form-create-designer/issues"
    },
    "homepage": "https://www.form-create.com",
    "publishConfig": {
        "access": "public"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,vue}": [
            "eslint --fix",
            "git add"
        ]
    },
    "peerDependencies": {
        "vue": "^2.7"
    },
    "dependencies": {
        "@form-create/component-wangeditor": "^2.5.15",
        "@form-create/element-ui": "^2.5.35",
        "@form-create/utils": "^2.5.35",
        "codemirror": "5.60.0",
        "element-ui": "^2.15.2",
        "vue": "^2.7.14",
        "vuedraggable": "^2.24.3"
    },
    "devDependencies": {
        "@vitejs/plugin-vue2": "^2.3.1",
        "jsonlint-mod": "^1.7.6",
        "vite": "^5.2.0",
        "vite-plugin-banner": "^0.7.1",
        "vite-plugin-css-injected-by-js": "^3.5.1"
    }
